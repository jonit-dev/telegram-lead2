{
  "workbench.colorTheme": "Activate UMBRA protocol",
  "files.associations": {
    "*.js": "javascriptreact",
    "*.jsx": "javascriptreact",
    "*.ts": "typescriptreact",
    "*.tsx": "typescriptreact"
  },
  "editor.defaultFormatter": "ms-vscode.vscode-typescript-tslint-plugin",
  "[javascript]": {
    "editor.defaultFormatter": "ms-vscode.vscode-typescript-tslint-plugin"
  },
  "eslint.enable": false,
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  }
}
